          "rawQuery": true,
          "rawSql": "WITH status_changes_with_next AS (\r\n    SELECT\r\n        user_id,\r\n        username,\r\n        status,\r\n        timestamp AS status_start_time,\r\n        LEAD(timestamp, 1, $__timeTo()) OVER (PARTITION BY user_id ORDER BY timestamp) AS next_status_time\r\n    FROM\r\n        channel_user_status\r\n    WHERE\r\n        $__timeFilter(timestamp)\r\n),\r\nonline_periods AS (\r\n    SELECT\r\n        user_id,\r\n        username,\r\n        status_start_time,\r\n        next_status_time,\r\n        CASE\r\n            WHEN next_status_time > status_start_time THEN\r\n                EXTRACT(EPOCH FROM (next_status_time - status_start_time))\r\n            ELSE 0\r\n        END AS duration_seconds\r\n    FROM\r\n        status_changes_with_next\r\n    WHERE\r\n        status = 'online'\r\n)\r\nSELECT\r\n    username,\r\n    SUM(duration_seconds) AS total_online_duration\r\nFROM\r\n    online_periods\r\nWHERE duration_seconds > 0\r\nGROUP BY\r\n    username\r\nORDER BY\r\n    total_online_duration DESC\r\nLIMIT 10;\r\n",
