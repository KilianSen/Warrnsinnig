        {
          "datasource": {
            "type": "postgres",
            "uid": "${DS_POSTGRESQL}"
          },
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "WITH user_latest_status AS (\n  SELECT\n    c1.user_id,\n    c1.status,\n    c1.timestamp\n  FROM channel_user_status c1\n  JOIN (\n    -- For each user and timestamp, find the max timestamp <= that timestamp\n    SELECT\n      user_id,\n      timestamp,\n      MAX(timestamp) OVER (PARTITION BY user_id ORDER BY timestamp ROWS BETWEEN UNBOUNDED PRECEDING AND CURRENT ROW) AS latest_timestamp\n    FROM channel_user_status\n    WHERE $__timeFilter(timestamp)\n  ) c2 ON c1.user_id = c2.user_id AND c1.timestamp = c2.latest_timestamp\n  WHERE $__timeFilter(c1.timestamp)\n),\ntime_buckets AS (\n  SELECT DISTINCT timestamp AS time FROM channel_user_status WHERE $__timeFilter(timestamp)\n)\nSELECT\n  tb.time,\n  COUNT(DISTINCT CASE WHEN uls.status = 'online' THEN uls.user_id END) AS online,\n  COUNT(DISTINCT CASE WHEN uls.status = 'away' THEN uls.user_id END) AS away,\n  COUNT(DISTINCT CASE WHEN uls.status = 'dnd' THEN uls.user_id END) AS dnd,\n  COUNT(DISTINCT CASE WHEN uls.status = 'offline' THEN uls.user_id END) AS offline\nFROM time_buckets tb\nLEFT JOIN user_latest_status uls ON uls.timestamp <= tb.time\nGROUP BY tb.time\nORDER BY tb.time;\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "Overall User Status Over Time",
      "type": "timeseries"
